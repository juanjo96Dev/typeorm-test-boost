{
  "name": "fastypest",
  "version": "1.4.1",
  "description": "Restores the database automatically after each test. Allows serial execution of tests without having to delete and restore the database having to stop the application",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=18.18.0"
  },
  "packageManager": "yarn@4.2.2",
  "volta": {
    "node": "18.18.0",
    "yarn": "4.2.2"
  },
  "scripts": {
    "postinstall": "husky install",
    "prebuild": "rimraf dist",
    "build": "tsc",
    "eslint": "eslint --fix .",
    "pretest": "tsc",
    "test:ci": "jest --runInBand --forceExit --verbose --showSeed --config ./tests/jest.config.json",
    "test": "yarn build && yarn test:ci",
    "docker": "ts-node -r tsconfig-paths/register scripts/prepare-docker.ts",
    "release": "standard-version --release-as patch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/juanjoGonDev/fastypest.git"
  },
  "keywords": [
    "typeorm",
    "test",
    "transaction",
    "rollback",
    "nestjs",
    "restore",
    "db",
    "speed",
    "up",
    "tests",
    "jest",
    "fastypest"
  ],
  "author": "juanjoGonDev",
  "license": "GPL",
  "bugs": {
    "url": "https://github.com/juanjoGonDev/fastypest/issues"
  },
  "homepage": "https://github.com/juanjoGonDev/fastypest#readme",
  "devDependencies": {
    "@eslint/js": "^9.3.0",
    "@types/cross-spawn": "^6.0.6",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.12",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "cross-spawn": "^7.0.3",
    "eslint": "^9.3.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^17.7.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.2.0",
    "globals": "^15.3.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "mariadb": "^3.3.0",
    "mysql": "^2.18.1",
    "pg": "^8.11.5",
    "rimraf": "^5.0.7",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.1.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.20",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.11.0"
  },
  "peerDependencies": {
    "typeorm": "^0.3.20"
  }
}
